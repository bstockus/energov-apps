          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PermitValuation">
          <DataField>PermitValuation</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="PermitAddress">
          <DataField>PermitAddress</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
