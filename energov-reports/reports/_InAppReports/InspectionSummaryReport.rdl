    <DataSet Name="InspectionsDataSet">
      <Query>
        <DataSourceName>EnerGov</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@INSPECTIONID">
            <Value>=Parameters!INSPECTIONID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH
    InspectionLinkedCases AS (
        SELECT
            1 AS LinkId, 'Plan' AS CaseModule, plpt.PLANNAME AS TypeName, plpwc.NAME AS SubTypeName, plp.PLPLANID AS CaseId, plp.PLANNUMBER AS CaseNumber
        FROM PLPLAN plp
        INNER JOIN PLPLANTYPE plpt ON plp.PLPLANTYPEID = plpt.PLPLANTYPEID
        INNER JOIN PLPLANWORKCLASS plpwc ON plp.PLPLANWORKCLASSID = plpwc.PLPLANWORKCLASSID

        UNION ALL SELECT
            2 AS LinkId, 'Permit' AS CaseModule, pmpt.NAME AS TypeName, pmpwc.NAME AS SubTypeName, pmp.PMPERMITID AS CaseId, pmp.PERMITNUMBER AS CaseNumber
        FROM PMPERMIT pmp
        INNER JOIN PMPERMITTYPE pmpt ON pmp.PMPERMITTYPEID = pmpt.PMPERMITTYPEID
        INNER JOIN PMPERMITWORKCLASS pmpwc ON pmp.PMPERMITWORKCLASSID = pmpwc.PMPERMITWORKCLASSID

        UNION ALL SELECT
            3 AS LinkId, 'Code' AS CaseModule, cmct.NAME AS TypeName, '' AS SubTypeName, cmcc.CMCODECASEID AS CaseId, cmcc.CASENUMBER AS CaseNumber
        FROM CMCODECASE cmcc
        INNER JOIN CMCASETYPE cmct ON cmcc.CMCASETYPEID = cmct.CMCASETYPEID)

SELECT
    insp.IMINSPECTIONID AS InspectionId,
    inspType.NAME AS InspectionType,
    inspStatus.DESCRIPTION AS InspectionStatus,
    insp.INSPECTIONNUMBER AS InspectionNumber,
    insp.REQUESTEDDATE AS RequestedDate,
    insp.SCHEDULEDSTARTDATE AS ScheduledDate,
    insp.ACTUALSTARTDATE AS ActualDate,
    insp.COMMENTS AS InspectionComments,
    ilc.CaseId AS LinkedCaseId,
    ilc.TypeName AS LinkedCaseTypeName,
    ilc.SubTypeName AS LinkedCaseSubTypeName,
    ilc.CaseModule AS LinkedCaseModule,
    ilc.CaseNumber AS LinkedCaseNumber
FROM IMINSPECTION insp
INNER JOIN IMINSPECTIONTYPE inspType ON insp.IMINSPECTIONTYPEID = inspType.IMINSPECTIONTYPEID
INNER JOIN IMINSPECTIONSTATUS inspStatus ON insp.IMINSPECTIONSTATUSID = inspStatus.IMINSPECTIONSTATUSID
LEFT OUTER JOIN InspectionLinkedCases ilc ON ilc.LinkId = insp.IMINSPECTIONLINKID AND ilc.CaseId = insp.LINKID
WHERE insp.IMINSPECTIONID = @INSPECTIONID;</CommandText>
      </Query>
