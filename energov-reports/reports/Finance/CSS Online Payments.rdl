  <DataSets>
    <DataSet Name="ReceiptsDataSet">
      <Query>
        <DataSourceName>EnerGovCss</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@InvoiceNumber">
            <Value>=Parameters!InvoiceNumber.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
    RECEIPT_ID,
    AUTHORIZATION_CODE,
    REFERENCE_NUMBER,
    rcpt.AMOUNT AS PaymentAmount,
    PRI.AMOUNT AS InvoiceAmount,
    TRANSACTION_DATE,
    TRANSACTION_TYPE,
    USER_ID,
    TENANT_ID,
    PAID,
    ERROR_MESSAGE,
    REQUEST_DATE,
    ITEM_DATA
FROM RECEIPT rcpt
INNER JOIN PAYMENT_REQUEST PR on rcpt.PAYMENT_REQUEST_ID = PR.PAYMENT_REQUEST_ID
INNER JOIN PAYMENT_REQUEST_ITEM PRI on PR.PAYMENT_REQUEST_ID = PRI.PAYMENT_REQUEST_ID
WHERE ((@InvoiceNumber IS NULL OR LEN(@InvoiceNumber) = 0) OR
      ITEM_DATA LIKE '%"Invoice":"' + @InvoiceNumber + '"%') AND
      ((@StartDate IS NULL OR @EndDate IS NULL) OR rcpt.TRANSACTION_DATE BETWEEN @StartDate AND @EndDate + 1)</CommandText>
      </Query>
